  <script>
    document.getElementById('CustomerLoginForm').addEventListener('submit', function (event) {
      event.preventDefault(); // Prevent form submission

      // Clear previous error messages
      const errorMessagesDiv = document.getElementById('errorMessages');
      errorMessagesDiv.innerHTML = '';
      errorMessagesDiv.style.display = 'none';

      // Get form values
      const email = document.getElementById('CustomerEmail').value.trim();
      const password = document.getElementById('CustomerPassword').value;

      // Validation flags
      let valid = true;
      let errorMessage = '';

      // Validate Email
      const emailPattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
      if (!email || !emailPattern.test(email)) {
        valid = false;
        errorMessage += 'Valid email is required.<br>';
      }

      // Validate Password
      if (!password) {
        valid = false;
        errorMessage += 'Password is required.<br>';
      }

      // Show errors or submit form
      if (valid) {
        this.submit(); // Submit the form if valid
      } else {
        errorMessagesDiv.innerHTML = errorMessage; // Show error messages
        errorMessagesDiv.style.display = 'block'; // Make the error messages visible
      }
    });
  </script>

