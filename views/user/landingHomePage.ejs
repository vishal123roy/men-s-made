            document.addEventListener('DOMContentLoaded', function () {
                var productPrices = document.querySelectorAll('.product-price');

                productPrices.forEach(function (priceDiv) {
                    var oldPrice = priceDiv.querySelector('.old-price');
                    var newPrice = priceDiv.querySelector('.price');

                    if (oldPrice && newPrice) {
                        var oldPriceValue = parseFloat(oldPrice.textContent.trim());
                        var newPriceValue = parseFloat(newPrice.textContent.trim());

                        if (oldPriceValue === newPriceValue) {
